class Solution
{
    public void transpose(int n,int a[][])
    {
        int b[][]=new int[n][n];
        for(int i=0;i<a.length;i++)
        {
            for(int j=0;j<a.length;j++)
            {
                b[j][i]=a[i][j];
            }
        }
        
        for(int i=0;i<a.length;i++)
        {
            for(int j=0;j<a.length;j++)
            {
                a[i][j]=b[i][j];
            }
        }
    }
}
